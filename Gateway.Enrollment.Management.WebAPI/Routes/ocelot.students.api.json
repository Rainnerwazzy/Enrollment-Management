      "SwaggerKey": "students",
      "UseServiceDiscovery": true,
      "ServiceDiscoveryProvider": {
        "Host": "enrollment.management.students.api",
        "Port": 4204,
        "Type": "Eureka"
      },
      "ServiceName": "SERVICE.ENROLLMENTMANAGEMENTSTUDENTSAPI",
      "UpstreamPathTemplate": "/students/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ]
}
